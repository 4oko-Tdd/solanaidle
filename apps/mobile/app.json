{
  "expo": {
    "name": "Seeker Node",
    "slug": "seeker-node",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "seekernode",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0a1628",
      "translucent": false
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0a1628"
    },
    "android": {
      "package": "com.seekernode.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "seekernode"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "./plugins/fix-build-gradle",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "kotlinVersion": "2.0.21"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ae7d3ece-8210-41f9-978f-d412ac78b6d7"
      }
    }
  }
}
